{
  "name": "jupyterlab_twitterfollow",
  "version": "0.1.0",
  "description": "Adds a Markdown cell into your notebook to let others follow you on Twitter",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/polong-lin/jupyterlab_twitterfollow",
  "bugs": {
    "url": "https://github.com/polong-lin/jupyterlab_twitterfollow/issues"
  },
  "license": "BSD-3-Clause",
  "author": "polong-lin",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/polong-lin/jupyterlab_twitterfollow.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@angular/core": "^7.1.0",
    "@angular/http": "^7.1.0",
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1",
    "@jupyterlab/docregistry": "^0.19.1",
    "@jupyterlab/notebook": "^0.19.2",
    "@phosphor/disposable": "^1.1.2",
    "@types/request": "^2.48.1",
    "request-promise-native": "^1.0.5",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.1.1"
  },
  "jupyterlab": {
    "extension": true
  }
}
